  "strategy_selection_rules": {
    "default_strategy": "oracle_full_pipeline",
    
    "complexity_thresholds": {
      "simple": {
        "max_tables": 1,
        "max_conditions": 2,
        "requires_aggregation": false
      },
      "medium": {
        "max_tables": 3,
        "max_conditions": 5,
        "requires_aggregation": true
      },
      "complex": {
        "max_tables": null,
        "max_conditions": null,
        "requires_aggregation": true
      }
    },
    
