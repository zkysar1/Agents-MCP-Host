{
  "strategies": {
    "oracle_full_pipeline": {
      "name": "Oracle Full Database Q&A Pipeline",
      "description": "Complete pipeline for answering questions using Oracle database data",
      "host": "oracledbanswerer",
      "complexity": "complex",
      "steps": [
        {
          "step": 1,
          "tool": "evaluate_query_intent",
          "server": "query-intent",
          "description": "Analyze query intent and complexity"
        },
        {
          "step": 2,
          "tool": "analyze_query",
          "server": "oracle-query-analysis",
          "description": "Perform semantic analysis of the natural language query"
        },
        {
          "step": 3,
          "tool": "match_oracle_schema",
          "server": "oracle-schema-intel",
          "description": "Find relevant tables and columns using fuzzy+LLM matching"
        },
        {
          "step": 4,
          "tool": "map_business_terms",
          "server": "business-map",
          "description": "Map business terminology to schema elements",
          "optional": true
        },
        {
          "step": 5,
          "tool": "generate_oracle_sql",
          "server": "oracle-sql-gen",
          "description": "Generate SQL query from analysis and schema matches"
        },
        {
          "step": 6,
          "tool": "optimize_oracle_sql",
          "server": "oracle-sql-gen",
          "description": "Optimize the generated SQL if needed",
          "optional": true
        },
        {
          "step": 7,
          "tool": "validate_oracle_sql",
          "server": "oracle-sql-val",
          "description": "Validate SQL against schema and permissions"
        },
        {
          "step": 8,
          "tool": "run_oracle_query",
          "server": "oracle-db",
          "description": "Execute the validated SQL query"
        },
        {
          "step": 9,
          "tool": "translate_enum",
          "server": "business-map",
          "description": "Translate any enum codes in results",
          "optional": true
        },
        {
          "step": 10,
          "tool": "format_results",
          "server": "oracle-db",
          "description": "Format results into natural language response",
          "optional": true
        }
      ],
      "capabilities_required": [
        "database_access",
        "llm_analysis",
        "schema_intelligence",
        "sql_generation"
      ]
    },
    
